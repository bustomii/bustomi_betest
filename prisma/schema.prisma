generator client {
  provider = "prisma-client-js"
}

// mongodb datasource
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserData {
  id             Bytes  @id @default(auto()) @map("_id") @db.ObjectId
  userName       String
  accountNumber  String
  emailAddress   String @unique
  identityNumber String
}

model UserAuth {
  id       Bytes  @id @default(auto()) @map("_id") @db.ObjectId
  userName String @unique
  password String
}
